# Network Configuration
# Options: mainnet, testnet, previewnet
VITE_NETWORK=testnet

# Hedera Account for admin interactions (wallet that deployed VoucherModuleHTS contract)
VITE_HAPI_ADMIN="0.0.xxxxxx"

# WalletConnect Project ID
# Required for WalletConnect integration. Get yours at https://cloud.walletconnect.com
VITE_WC_PROJECT_ID=your_wallet_connect_project_id

# Hedera Network Configuration
# EVM RPC endpoint for Hedera network
VITE_EVM_RPC=https://testnet.hashio.io/api
# Chain ID for Hedera Testnet (296)
VITE_EVM_CHAIN_ID=296

# Explorer Configuration
# Base URL for transaction explorer (HashScan)
EXPLORER_TX=https://hashscan.io/testnet/transaction/

# Hedera Mirror Node
# API endpoint for querying Hedera network state
VITE_MIRROR=https://testnet.mirrornode.hedera.com/api/v1

# Smart Contract Addresses
# Voucher Module contract address
VITE_VOUCHER_MODULE=your_voucher_module_address
# Virtual OFD token address
VITE_VOFD=your_vofd_address
# Optional: VOFD token ID in numeric format
# VITE_VOFD_ID=0.0.x

# Hedera OFD Token (hOFD)
# ERC20 token address used for sponsorship and issuance
VITE_HOFD=your_hofd_address

# Deployment Information
# Block number where Voucher contract was deployed
VITE_VOUCHER_DEPLOY_BLOCK=0
# Admin account Hedera ID
VITE_ADMIN_HEDERA_ID=your_admin_hedera_id

# Token Addresses
# Wrapped HBAR token address
VITE_WHBAR=your_whbar_address
# MintingHub contract address
VITE_MINTING_HUB=your_minting_hub_address

# Collateral Configuration
# JSON array of supported collateral tokens with their details
VITE_COLLATERALS=[{"label": "WHBAR - 8d","symbol": "WHBAR","address": "your_whbar_address","decimals": 8,"logo": "logo_url"}]